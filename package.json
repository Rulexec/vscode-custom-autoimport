{
	"name": "custom-autoimport",
	"displayName": "Custom Auto Import",
	"description": "Provides ability to declare custom names for packages autoimport",
	"license": "MIT",
	"version": "0.0.3",
	"publisher": "rulexec",
	"repository": "https://github.com/Rulexec/vscode-custom-autoimport",
	"engines": {
		"vscode": "^1.42.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onLanguage:javascript",
		"onLanguage:javascriptreact",
		"onLanguage:typescript",
		"onLanguage:typescriptreact"
	],
	"contributes": {
		"configuration": {
			"title": "CustomAutoImport",
			"properties": {
				"customAutoImport": {
					"type": "object",
					"properties": {
						"imports": {
							"type": "object",
							"description": "Mapping of variable name to module name",
							"patternProperties": {
								"^.*$": {
									"type": "string",
									"minLength": 1
								}
							}
						}
					}
				}
			}
		}
	},
	"main": "./out/extension.js",
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "webpack",
		"test": "./node_modules/.bin/mocha -r esm src/**/*.test.js"
	},
	"devDependencies": {
		"chai": "^4.2.0",
		"deep-equal-in-any-order": "^1.0.28",
		"esm": "^3.2.25",
		"mocha": "^8.1.3",
		"webpack": "^4.44.2",
		"webpack-cli": "^3.3.12"
	}
}
