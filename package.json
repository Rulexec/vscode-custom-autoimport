{
	"name": "custom-autoimport",
	"displayName": "Custom Auto Import",
	"description": "Provides ability to declare custom names for packages autoimport",
	"license": "MIT",
	"version": "0.0.3",
	"publisher": "rulexec",
	"repository": "https://github.com/Rulexec/vscode-custom-autoimport",
	"engines": {
		"vscode": "^1.42.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onLanguage:javascript",
		"onLanguage:typescript"
	],
	"contributes": {
		"configuration": {
			"title": "CustomAutoImport",
			"properties": {
				"customAutoImport": {
					"type": "object",
					"properties": {
						"imports": {
							"type": "object",
							"description": "Mapping of variable name to module name",
							"patternProperties": {
								"^.*$": {
									"type": "string",
									"minLength": 1
								}
							}
						}
					}
				}
			}
		}
	},
	"main": "./out/extension.js",
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "tslint -p ./",
		"watch": "tsc -watch -p ./"
	},
	"devDependencies": {
		"@types/node": "^10.14.17",
		"@types/vscode": "^1.34.0",
		"tslint": "^5.16.0",
		"typescript": "^3.5.1"
	}
}
